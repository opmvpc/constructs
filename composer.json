{
    "name": "opmvpc/constructs",
    "description": "Data structures",
    "keywords": ["data structures", "package"],
    "homepage": "https://github.com/opmvpc/constructs",
    "license": "MIT",
    "authors": [
        {
            "name": "opmvpc",
            "email": "thibsix@outlook.be",
            "homepage": "https://tsix.be",
            "role": "Developer"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.2",
        "symfony/var-dumper": "^4.3"
    },
    "require-dev": {
		"friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "vimeo/psalm": "^3.11",
        "nunomaduro/phpinsights": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Opmvpc\\Constructs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opmvpc\\Constructs\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "psalm",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
